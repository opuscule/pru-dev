<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
   
 <style>
        :root {
            --color-bg-light: #efebdc;
            --headline1-font-size: clamp(3.75rem,2.1875rem + 5.2083vw,6.875rem);
            --font_header: "Amatic SC",bradley hand,cursive;
            --font-body: "Open Sans", arial, sans-serif;
            --color-body-copy: #666;
            --headline2-font-size: clamp(2rem,0.5rem + 5vw,5rem);
            --headline3-font-size: clamp(2rem, 1.6875rem + 1.0417vw, 2.625rem);
            --color-bg-extralight: #faf9f4;
            --color-bg-main: #f6f3ea;
            --color-primary-red: #ee6a5b;
            --border-radius: .3rem;
        }

        .page_bg {
          background-color: var(--color-bg-light);
         }
         .h1-fs{
           font-size: var(--headline1-font-size);
         }
         .h1-fs, .h2-fs {
            font-weight: 700;
            line-height: 1;
            text-wrap: pretty;
         }
         h1, h2, h3 {
            font-family: var(--font_header);
            font-weight: 400;
         }
         li, ol, p, ul {
            color: var(--color-body-copy);
            font-family: var(--font-body);
         }
         .h2-fs {
            font-size: var(--headline2-font-size);
         }
         .h3-fs {
            font-size: var(--headline3-font-size);
            font-weight: 700;
            line-height: 1;
         }
         .wspbgadj {
            margin-bottom: -1px;
         }
         .extralightbg {
            background-color: var(--color-bg-extralight);
         }
         .footer_overlap {
            margin-bottom: -200px;
            padding-bottom: 100px;
         }
         .pup_card_bg {
            background-color: var(--color-bg-main);
            padding-top: 4rem;
         }
         .top-left-rounded-wrapper {
            background-color: var(--color-bg-light);
            border-radius: 12.5vw 0 0 0;
            margin: 0 0 0 4vw;
            padding: 4rem 0;
         }
         .font-color-red {
              color: var(--color-primary-red);
          }

          a {
                color: var(--color-primary-red);
            }
            .contact_us_sm {
              background-color: hsla(0, 0%, 100%, .6) !important;
              height: 3rem;
              width: 3rem;
            }

            .contact_us_sm.contact_us_fb svg {
                transition: all .25s ease;
                width: .8rem;
            }
            .contact_us_sm.contact_us_ig svg {
                transition: all .25s 
            ease;
                width: 1.1rem;
            }
            .contact_us_sm.contact_us_yt svg {
                transition: all .25s ease;
                width: 1.3rem;
            }
            .bg-secondary{
              background-color:oklch(.7057 .158 124.46) !important;
            }
            .contact_us_sm:focus svg, .contact_us_sm:hover svg {
              fill: var(--color-primary-red);
            }
        </style>



